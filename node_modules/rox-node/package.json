{
  "name": "rox-node",
  "version": "5.4.12",
  "description": "Rollout.io ROX JS SDK Node",
  "author": "Rollout.io <support@rollout.io>",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://rollout.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/rollout/roxjs.git"
  },
  "bugs": {
    "url": "https://github.com/rollout/roxjs/issues",
    "email": "support@rollout.io"
  },
  "keywords": [
    "feature-flag",
    "feature-flags",
    "rollout",
    "rox",
    "flags",
    "flag",
    "configuration",
    "remote-control",
    "rollout.io"
  ],
  "scripts": {
    "lint": "eslint src",
    "start": "nodemon demo/index.js",
    "debug": "node --inspect-brk=5859 demo/index.js",
    "test": "BABEL_ENV=test jest --no-watchman --forceExit --testPathPattern ./src/**",
    "test:watch": "BABEL_ENV=test jest --testPathPattern ./src/** --watch",
    "prebuild": "yarn run clean:dist",
    "build": "NODE_ENV=production rollup --config=./rollup.config.js",
    "prepublishOnly": "yarn run test && yarn run build",
    "clean:dist": "rm -rf dist/*"
  },
  "ROX": {
    "api_version": "1.9.0"
  },
  "main": "dist/rox-node.js",
  "files": [
    "dist/*.js"
  ],
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@cloudbees/eslint-plugin": "cloudbees/eslint-plugin",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eventsource": "^2.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "rollup": "^2.67.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rox-crypto": "^5.4.12",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "uuid": "^8.0.0",
    "webpack": "^5.71.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "/node_modules/(?!rox-base).+\\.ts$": "ts-jest"
    },
    "moduleNameMapper": {
      "rox-base": "<rootDir>/../rox-base/src"
    },
    "preset": "ts-jest/presets/js-with-ts"
  },
  "gitHead": "75b020764a4eda65eda370487e24c941e5ac2937"
}
