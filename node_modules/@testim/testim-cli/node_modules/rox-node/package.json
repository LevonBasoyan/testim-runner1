{
  "name": "rox-node",
  "version": "4.9.18",
  "description": "Rollout.io ROX JS SDK Node",
  "author": "Rollout.io <support@rollout.io>",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://rollout.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/rollout/roxjs.git"
  },
  "bugs": {
    "url": "https://github.com/rollout/roxjs/issues",
    "email": "support@rollout.io"
  },
  "keywords": [
    "feature-flag",
    "feature-flags",
    "remote-configuration",
    "remote-variable",
    "rollout",
    "rox",
    "flags",
    "flag",
    "configuration",
    "remote-control",
    "rollout.io"
  ],
  "scripts": {
    "lint": "eslint src",
    "start": "nodemon demo/index.js",
    "debug": "node --inspect-brk=5859 demo/index.js",
    "test": "BABEL_ENV=test jest --no-watchman --testPathPattern ./src/**",
    "test:watch": "BABEL_ENV=test jest --testPathPattern ./src/** --watch",
    "prebuild": "yarn run clean:dist",
    "build": "NODE_ENV=production rollup --config=./rollup.config.js",
    "prepublishOnly": "yarn run test && yarn run build",
    "clean:dist": "rm -rf dist/*"
  },
  "ROX": {
    "api_version": "1.8.0"
  },
  "main": "dist/rox-node.js",
  "files": [
    "dist/*.js"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.3.0",
    "jest": "^22.1.2",
    "nodemon": "^1.19.4",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rox-base": "^4.9.18",
    "supertest": "^4.0.2",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "uuid": "^3.3.2",
    "webpack": "^3.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!rox-base).+\\.js$"
    ],
    "moduleNameMapper": {
      "rox-base": "<rootDir>/../rox-base/src"
    }
  },
  "gitHead": "d4a6e9472f360193b27829a75b3a78c7fb97d059"
}
